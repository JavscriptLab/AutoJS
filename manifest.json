{
  "manifest_version": 2,
  "name": "AutoJS",
  "version": "1.1",
  "background": {
    "scripts": ["main.js"],
    "persistent": true
  },
  "permissions": ["https://*/*",
    "http://*/*",
    "tabs","management","debugger","activeTab","storage"],
  "minimum_chrome_version": "60",
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "all_frames":true,
      "js":[ "inject.js"]
    }
  ],
  "web_accessible_resources": [
    "js/*"
  ]
}
